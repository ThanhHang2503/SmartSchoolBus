{
    "version": 3,
    "framework": "nextjs",
    "build": {
      "env": {
        "NODE_VERSION": "20"
      }
    },
    "regions": ["sin1"],
    "functions": {
      "frontend/app/api/**/*": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/frontend/$1"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }