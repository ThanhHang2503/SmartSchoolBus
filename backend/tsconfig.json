{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": false
  },
  "include": [
    "models/**/*.ts",
    "hardcode_data/**/*.ts",
    "student/**/*.ts",
    "*.ts"
  ],
  "exclude": ["node_modules"]
}